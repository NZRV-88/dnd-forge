{
    "buildCommand": "npm run build:vercel",
    "outputDirectory": "dist/spa",
    "functions": {
        "api/index.ts": {
            "runtime": "nodejs18.x"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
